@using CvMakerApp.Entity.Entities
@model Testimonial
@{
    ViewData["Title"] = "AddTestimonial";
    Layout = "~/Views/Admin/Index.cshtml";
}
<h3>Referans Ekle</h3>
<br />
<form method="post">
    <label>Referans: </label>
    <input type="text" class="form-control" asp-for="TestimonialName" /><br />
    <label>Pozisyon: </label>
    <input type="text" class="form-control" asp-for="Position" /><br />
    <label>Şirket: </label>
    <input type="text" class="form-control" asp-for="Company" /><br />
    <button type="submit" class="btn btn-primary">Ekle</button>
</form>




